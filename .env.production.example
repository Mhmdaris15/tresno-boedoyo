# Production Environment Variables
# Copy this file to .env.production and fill in your actual values

# Database Configuration
PROD_DATABASE_URL=postgresql://tresno_user:your_password@postgres:5432/tresno_boedoyo_prod
PROD_DB_USER=tresno_user
PROD_DB_PASSWORD=your_ultra_secure_password
PROD_DB_REPLICATION_PASSWORD=your_replication_password

# JWT Configuration
PROD_JWT_SECRET=your_super_secret_jwt_key_production

# API URLs
PROD_API_URL=https://api.tresno-boedoyo.com
PROD_RPC_URL=https://mainnet.infura.io/v3/your_infura_project_id

# Web3 Configuration
PROD_WEB3_PRIVATE_KEY=your_web3_private_key_production

# Grafana
GRAFANA_ADMIN_PASSWORD=your_grafana_admin_password

# Docker Registry
GITHUB_TOKEN=your_github_personal_access_token
GITHUB_USERNAME=your_github_username
