# Staging Environment Variables
# Copy this file to .env.staging and fill in your actual values

# Database Configuration
STAGING_DATABASE_URL=postgresql://tresno_user:your_password@postgres:5432/tresno_boedoyo_staging
STAGING_DB_USER=tresno_user
STAGING_DB_PASSWORD=your_secure_password

# JWT Configuration
STAGING_JWT_SECRET=your_super_secret_jwt_key_staging

# API URLs
STAGING_API_URL=https://api-staging.tresno-boedoyo.com
STAGING_RPC_URL=https://sepolia.infura.io/v3/your_infura_project_id

# Web3 Configuration
STAGING_WEB3_PRIVATE_KEY=your_web3_private_key_staging

# Grafana
GRAFANA_ADMIN_PASSWORD=your_grafana_admin_password

# Docker Registry
GITHUB_TOKEN=your_github_personal_access_token
GITHUB_USERNAME=your_github_username
